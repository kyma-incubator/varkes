    "loopback": "^3.25.0",
    "loopback-boot": "^3.0.0",
    "loopback-component-explorer": "^6.3.1",
    "loopback-connector": "^4.6.1",
    "morgan": "^1.9.1",
    "n-odata-server": "^0.4.1",
    "odata2openapi": "^1.3.0",
    "strong-error-handler": "^3.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "copyfiles": "^2.1.0",
    "mocha": "^6.0.2",
    "supertest": "^3.4.2",
    "typescript": "^3.3.3333"
  },
  "repository": {
