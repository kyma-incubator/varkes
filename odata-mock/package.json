  "main": "dist/server/app.js",
  "types": "dist/app.d.ts",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "compile": "tsc && cd src && copyfiles test/services.xml ../dist/ && copyfiles test/varkes_config.json ../dist/ && copyfiles server/resources/* ../dist/",
