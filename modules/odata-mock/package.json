{
  "name": "varkes-odata-mock",
  "version": "0.12.0",
  "main": "dist/server/app.js",
  "types": "dist/server/types.d.ts",
  "license": "Apache-2.0",
  "description": "OData Mock",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "compile": "tsc && copyfiles -f -e '**/*.ts' src/test/* dist/test && copyfiles -f src/server/resources/* dist/server/resources",
    "start": "node dist/server/server.js",
    "start:dev": "npm run clean && npm run compile && export DEBUG=true && node dist/server/server ../test/varkes_config.json",
    "pretest": "npm run compile",
    "test": "mocha dist/test",
    "clean": "rm -rf dist || true && rm -r logs || true"
  },
  "dependencies": {
    "varkes-configuration": "^0.12.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "loopback": "^3.27.0",
    "loopback-boot": "^3.3.1",
    "loopback-component-explorer": "^6.5.1",
    "loopback-connector": "^5.0.1",
    "n-odata-server": "^0.4.1",
    "odata2openapi": "^1.3.2",
    "strong-error-handler": "^4.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.3",
    "copyfiles": "^2.4.0",
    "mocha": "^8.2.1",
    "supertest": "^6.0.1",
    "typescript": "~4.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kyma-incubator/varkes.git",
    "directory": "modules/odata-mock"
  }
}
