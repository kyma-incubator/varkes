    "topics": {
        "customer.created.v1": {
            "subscribe": {
                "summary": "Customer Register Event v2",
                "payload": {
                    "type": "object",
                    "required": [
                        "storeUid",
                        "customerId",
                        "customerUid"
                    ],
                    "properties": {
                        "customerUid": {
                            "type": "string",
                            "required": null,
                            "example": "adam@hybris.com",
                            "properties": null,
                            "description": "Email of a Customer",
                            "title": "Customer uid"
                        },
                        "storeUid": {
                            "type": "string",
                            "required": null,
                            "example": "4caad296",
                            "properties": null,
                            "description": "Unique id of a Store",
                            "title": "StoreUid"
                        },
                        "customerId": {
                            "type": "string",
                            "required": null,
                            "example": "0ed118f9474e",
                            "properties": null,
                            "description": "Unique id of a Customer",
                            "title": "Customer uid"
                        }
                    }
                }
            }
        },
        "password.forgotten.v1": {
            "subscribe": {
                "summary": "Forgotten Password Event v1",
                "payload": {
                    "type": "object",
                    "required": [
                        "storeUid",
                        "customerUid",
                        "customerId"
                    ],
                    "properties": {
                        "customerUid": {
                            "type": "string",
                            "required": null,
                            "example": "adam@hybris.com",
                            "properties": null,
                            "description": "Email of a Customer",
                            "title": "Customer uid"
                        },
                        "storeUid": {
                            "type": "string",
                            "required": null,
                            "example": "4caad296",
                            "properties": null,
                            "description": "Unique id of a Store",
                            "title": "StoreUid"
                        },
                        "customerId": {
                            "type": "string",
                            "required": null,
                            "example": "0ed118f9474e",
                            "properties": null,
                            "description": "Unique id of a Customer",
                            "title": "Customer uid"
                        },
                        "token": {
                            "type": "string",
                            "required": null,
                            "example": "0edED312318f9474e",
                            "properties": null,
                            "description": "Encrypted SecureToken",
                            "title": "Encrypted SecureToken"
                        }
                    }
                }
            }
        }
    }
