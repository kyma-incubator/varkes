{
    "spec": {
        "asyncapi": "1.0.0",
        "info": {
            "title": "ec-all-events",
            "version": "v1",
            "description": "EC Events v1"
        },
        "topics": {
            "customer.created.v1": {
                "subscribe": {
                    "summary": "Customer Register Event v2",
                    "payload": {
                        "type": "object",
                        "required": [
                            "storeUid",
                            "customerId",
                            "customerUid"
                        ],
                        "properties": {
                            "customerUid": {
                                "type": "string",
                                "required": null,
                                "example": {
                                    "customerUid": "adam@hybris.com"
                                },
                                "properties": null,
                                "description": "Email of a Customer",
                                "title": "Customer uid"
                            },
                            "storeUid": {
                                "type": "string",
                                "required": null,
                                "example": {
                                    "storeUid": "4caad296"
                                },
                                "properties": null,
                                "description": "Unique id of a Store",
                                "title": "StoreUid"
                            },
                            "customerId": {
                                "type": "string",
                                "required": null,
                                "example": {
                                    "customerUid": "0ed118f9474e"
                                },
                                "properties": null,
                                "description": "Unique id of a Customer",
                                "title": "Customer uid"
                            }
                        }
                    }
                }
            }
        }
    }
}